{
  "env": {
    "es2021": true,
    "react-native/react-native": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "semi": ["error", "always"],
    "prettier/prettier'": "error",
    "import/prefer-default-export": "off",
    "no-console": "warn",
    "no-unused-vars": "warn"
  },
  "extends": [
    "eslint:recommended", 
    "prettier", 
    "plugin:react/recommended"
  ],
  "plugins": [
    "react", 
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  }
}
